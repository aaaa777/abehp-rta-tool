{
    "manifest_version": 3,
    "name": "abehiroshi-HP-rta-tool",
    "description": "阿部寛のHP逆RTAの計測用ツールです",
    "short_name": "abe-rta-tool",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "http://abehiroshi.la.coocan.jp/*"
            ],
            "js": [
                "cs.js"
            ]
        },
        {
            "matches": [
                "https://www.google.com/search*"
            ],
            "js": [
                "g-cs.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}